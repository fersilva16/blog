{
  "name": "fersilva.dev",
  "description": "Personal Website",
  "author": "Fernando Silva <fernandonsilva16@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fersilva16/fersilva.dev.git"
  },
  "bugs": {
    "url": "https://github.com/fersilva16/fersilva.dev/issues"
  },
  "homepage": "https://github.com/fersilva16/fersilva.dev#readme",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "postinstall": "simple-git-hooks",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "dependencies": {
    "@chakra-ui/react": "1.8.0",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "framer-motion": "6.2.3",
    "next": "12.0.8",
    "next-intl": "2.3.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@emotion/babel-plugin": "11.7.2",
    "@next/eslint-plugin-next": "12.0.8",
    "@svgr/webpack": "6.2.0",
    "@types/node": "16.11.21",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "eslint": "8.7.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "nano-staged": "0.5.0",
    "prettier": "2.5.1",
    "simple-git-hooks": "2.7.0",
    "typescript": "4.5.5"
  },
  "nano-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "bash -c yarn type-check",
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "packageManager": "yarn@3.1.1",
  "dependenciesMeta": {
    "react-dom@17.0.2": {
      "unplugged": true
    },
    "react-icons@4.3.1": {
      "unplugged": true
    }
  }
}
